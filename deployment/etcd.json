{
  "etcd": {
    "dns-suffix": ".marathon.mesos",
    "framework-name": "etcd",
    "admin-port": "$PORT0",
    "cpus": 0.1,
    "mem": 128,
    "reseed-timeout": 240,
    "auto-reseed": true,
    "cpu-limit": 1,
    "mem-limit": 128,
    "disk-limit": 4096,
    "cluster-size": 1,
    "logging-verbosity": 1,
    "zk": "zk://master.mesos:2181/etcd"
  },
  "mesos": {
    "master": "zk://master.mesos:2181/mesos"
  }
}
