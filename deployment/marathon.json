{
  "id": "/dcos-networkobject-controller",
  "container": {
    "type": "MESO",
    "volumes": [
      {
        "containerPath": "/config",
        "hostPath": "/home/vagrant/.kube/config",
        "mode": "RO"
      }
    ],
    "docker": {
      "image": "wjglerum/dcos-networkobject-controller"
    }
  },
  "cpus": 0.1,
  "instances": 1,
  "mem": 128,
  "networks": [
    {
      "mode": "host"
    }
  ]
}
